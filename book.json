{
    "title": "Plataforma de Gestión de Residuos Gresst",
    "description": "Documentación completa de la plataforma Gresst para gestión integral de residuos",
    "author": "Gresst",
    "language": "es",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "search",
        "sharing",
        "fontsettings",
        "highlight",
        "livereload"
    ],
    "pluginsConfig": {
        "fontsettings": {
            "theme": "white",
            "family": "sans",
            "size": 2
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook",
                "twitter"
            ]
        }
    }
}
